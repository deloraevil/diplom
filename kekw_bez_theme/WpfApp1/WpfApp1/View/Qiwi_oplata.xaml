<Window x:Class="WpfApp1.View.Qiwi_oplata"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.View"
        mc:Ignorable="d"
        Title="Qiwi_oplata" Height="500" Width="1000"
        xmlns:wpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf">

    <Window.Style>
        <Style TargetType="Window" BasedOn="{StaticResource {x:Type Window}}"/>
    </Window.Style>

    <Grid>
        <Grid>
            <Border BorderThickness="2" BorderBrush="Black" Width="900" Height="350" Margin="0 0 0 60">
                <wpf:ChromiumWebBrowser x:Name="qwe"/>
            </Border>
            <Button Content="Open" VerticalAlignment="Center" HorizontalAlignment="Center" Width="357" Height="36" Margin="46,398,589,35" Command="{Binding Qiwi_add}" Click="Button_Click_1"/>
            <Button Content="Close" VerticalAlignment="Center" HorizontalAlignment="Center" Width="124" Height="36" Margin="822,398,46,35" Click="Button_Click"/>
        </Grid>
    </Grid>
</Window>
